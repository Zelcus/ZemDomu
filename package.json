{
  "name": "zemdomu",
  "displayName": "ZemDomu",
  "publisher": "Zelcus",
  "description": "A semantic linter for HTML and JSX/TSX",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zelcus/ZemDomu.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Linters"
  ],
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "zemdomu.lintWorkspace",
        "title": "ZemDomu: Scan Workspace for Semantic Issues"
      }
    ],
    "keybindings": [
      {
        "command": "zemdomu.lintWorkspace",
        "key": "ctrl+alt+z",
        "mac": "cmd+alt+z",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./tsconfig.json",
    "watch": "tsc -w",
    "package": "vsce package"
  },
  "devDependencies": {
  
    "@types/babel__traverse": "^7.20.7",
    "@types/htmlparser2": "^4.1.0",
    "@types/vscode": "^1.98.0",
    "@vscode/vsce": "^2.0.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "htmlparser2": "^10.0.0",
    "@babel/parser": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "@babel/types": "^7.27.0"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#2D2D2D",
    "theme": "dark"
  }
}